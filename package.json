{
  "name": "electron-autoupdate-example",
  "version": "0.2.3",
  "main": "main.js",
  "description": "electron autoupdate example project",
  "author": "Matt Haggard",
  "devDependencies": {
    "electron": "^1.4.15",
    "electron-builder": "^12.3.1"
  },
  "dependencies": {
    "electron-log": "^1.3.0"
  },
  "build": {
    "publish": {
      "provider": "generic",
      "url": "http://127.0.0.1:8080/update/${os}/${arch}",
      "channel": "latest"
    },
    "appId": "com.github.zapjelly.electronautoupdateexample",
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "nsis": {
      "perMachine": true
    }
  }
}